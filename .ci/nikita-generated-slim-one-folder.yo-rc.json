{
  "generator-nikita": {
    "name": "nikita-generated-slim-one-folder",
    "private": true,
    "template": "slim",
    "sassCompiler": "libSass",
    "features": [
      "webfonts",
      "svgBackgrounds",
      "gitinfos"
    ],
    "nikitaCssMixins": [
      "respond-to"
    ],
    "nikitaCssExtends": [
      "cf"
    ],
    "formFramework": false,
    "useBuildFolders": false,
    "cleanBuildFolders": false,
    "sourceFolder": "src/AppBundle/Resources/public",
    "requirejsPathToBowerComponents": "../../bower_components/"
  }
}
